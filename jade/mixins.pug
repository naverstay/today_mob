block vars
    - var prod = true;
    - var prod = false;

mixin dev_head(page_title)
    head
        title=page_title
        |
        | <!-- saved from url=(0014)about:internet -->
        meta(charset='utf-8')
        meta(name='viewport', content="width=device-width, initial-scale=1.0")
        meta(name="SKYPE_TOOLBAR", content="SKYPE_TOOLBAR_PARSER_COMPATIBLE")
        meta(name="apple-mobile-web-app-capable", content="yes")
        meta(name="format-detection", content="telephone=no")
        meta(name="theme-color", content="#3d0ba9")

        - if (prod)
            style
                include ../styles/header_global.css

mixin loop_while(counter)
    -i=0;
    while i < counter
        -i++;
        block

mixin action_list()
  ul.action_list
    li: a.action_link._w(href='#')
      - if (amp)
        .action_link_icon
          +svg_icon('tw')
      span Twitter
    li: a.action_link._t(href='#')
      - if (amp)
        .action_link_icon
          +svg_icon('tg')
      span Telegram
    li: a.action_link._f(href='#')
      - if (amp)
        .action_link_icon
          +svg_icon('fb')
      span Facebook
    li: a.action_link._r(href='#')
      - if (amp)
        .action_link_icon
          +svg_icon('rss')
      span RSS

mixin footer(white)
    .footer(class= white ? '_white' : '')
        .section_inner
            ul.footer_menu
                li: a.footer_link(href='#') News
                li: a.footer_link(href='#') Features
                li: a.footer_link(href='#') Sharks pool
                li: a.footer_link(href='#') Price analysis
                li: a.footer_link(href='#') Share insight
                li: a.footer_link(href='about.html') About us

            .footer_logo
                a.logo(href='index.html')
                    - if (amp)
                        amp-img(src= white ? 'i/logo_f_v.png' : 'i/logo_c.svg', width='198', height='113', alt='logo')
                    - else
                        img(src= white ? 'i/logo_f_v.png' : 'i/logo_c.svg')

            p Follow us with:

            +action_list()

        .go_top_holder.goTopHolder
            .section_inner
                a.go_top.goTop(href='#')

mixin top_info()
    .top_info
        .currency_switcher
            select.currency_select
                option &#8364;
                option &#36;
                option &#8381;
                option &#163;
            .select_arrow
                - if (amp)
                    +svg_icon('triangle')
            .select_overlay
        .currency_info
            .currency_info_table
                .currency_info_cell
                    p
                        span BTC
                        span.clr_red_2 (-8.82%)
                    p
                        span.currency_val 2636.79
                .currency_info_cell
                    p
                        span BCH
                        span.clr_green (+10.82%)
                    p
                        span.currency_val 3236.51
                .currency_info_cell
                    p
                        span ETH
                        span.clr_red_2 (-8.82%)
                    p
                        span.currency_val 26236.79

mixin main_menu(subscribe)
    - if (subscribe)
        .subscribe_holder
            a.subscribe_close.subscribeAsideBtn(href='#')
            .subscribeBlock.subscribe_block
                .subscribe_title You can get more hot news and ideas.

                .footer_form
                    form.validateMe.formParent.subscribeForm(action='#')
                        .form_cell.formCell
                            .input_w
                                input.input_v1.forceValidation(placeholder='Enter your email' class='validate[required]')

                        .form_cell
                            button.btn_v1.subscribe_btn.white_btn Subscribe

                .footer_msg._success
                    .footer_msg_title
                        span.icon-check.msg_icon
                        br
                        span Subscription<br>enabled

                .footer_msg._fail
                    .footer_msg_title
                        span.icon-close.clr_red.msg_icon
                        span Error
                    p Something went wrong,<br>try again

            .footer_action
                p Follow us with:

                +action_list()

    .main_menu_holder(layout= (amp ? 'nodisplay' : null))
        ul.main_menu
            li.menu_item
                a.menu_link(href='#') News
            li.menu_item
                a.menu_link(href='#') Features
            li.menu_item
                a.menu_link(href='#') Sharks pool
            li.menu_item
                a.menu_link(href='price.html') Price analysis
            li.menu_item
                a.menu_link(href='#') Share insight
            li.menu_item
                a.menu_link(href='about.html') About us

        .menu_footer.subscribeBlock
            .footer_subscribe
                a.subscribe_close.subscribeClose(href='#')
                a.subscribe_link.subscribeLink(href='#') Subscribe with your email

            .footer_action
                p Follow us with:

                +action_list()

            .footer_form
                form.validateMe.formParent.subscribeForm(action='#')
                    .form_cell.formCell
                        .input_w
                            input.input_v1.forceValidation(placeholder='Enter your email' class='validate[required]')

                    .form_cell
                        button.btn_v1.subscribe_btn.white_btn Subscribe

            .footer_msg._success
                .footer_msg_title
                    span.icon-check.msg_icon
                    br
                    span Subscription<br>enabled

            .footer_msg._fail
                .footer_msg_title
                    span.icon-close.clr_red.msg_icon
                    span Error
                p Something went wrong,<br>try again

    .sh_holder
        .search_title Share
        a.menu_close_btn.actionBtn(href='#')
        .action_block
            .action_target
                .action_img
                    img(src='i/action_img.jpg')
                .action_title US State Regulators Hitt BitConnect With Second Cease-And-Desist

            ul.action_soc
                li: a.action_sh._f(href='#' data-network="facebook" class='st-custom-button')
                    span.action_sh_text Facebook
                li: a.action_sh._t(href='#' data-network="twitter" class='st-custom-button')
                    span.action_sh_text Twitter
                li: a.action_sh._rd(href='#' data-network="reddit" class='st-custom-button')
                    span.action_sh_text Reddit
