                li: a.action_sh._i(href='#' data-network="instagram" class='st-custom-button')
                    span.action_sh_text Instagram
                li: a.action_sh._p(href='#' data-network="pinterest" class='st-custom-button')
                    span.action_sh_text Pinterest
                li: a.action_sh._s(href='#' data-network="pinterest" class='st-custom-button')
                    span.action_sh_text Pinterest
                li: a.action_sh._tm(href='#' data-network="tumblr" class='st-custom-button')
